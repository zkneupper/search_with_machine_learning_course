{
  "featureset": {
    "features": [
      {
        "name": "click_prior",
        "template_language": "mustache",
        "template": {
          "query_string": {
            "query": "{{click_prior_query}}",
            "fields": ["_id"]
          }
        }
      },
      {
        "name": "name_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "name": "{{keywords}}"
          }
        }
      },
      {
        "name": "name_phrase_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match_phrase": {
            "name": "{{keywords}}"
          }
        }
      },
      {
        "name": "salesRankShortTerm",
        "template_language": "mustache",
        "template": {
          "function_score": {
            "functions": [
              {
                "field_value_factor": {
                  "field": "salesRankShortTerm",
                  "missing": 1000000
                }
              }
            ],
            "query": {
              "match_all": {}
            }
          }
        }
      },
